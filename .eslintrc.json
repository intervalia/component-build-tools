{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "eslint-plugin-html"
  ],
  "env": { "es6": true, "node": true, "browser": true },
  "globals": {
    "it": true,
    "describe": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "expect": true,
    "FS": true
  },
  "rules": {
    "block-scoped-var": 0,
    "brace-style": [0, { "allowSingleLine": true }],
    "camelcase": 1,
    "comma-dangle": 2,
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": [1, "last"],
    "complexity": [1, 15],
    "consistent-return": 0,
    "consistent-this": [1, "_this"],
    "curly": [1, "multi-line"],
    "default-case": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "func-names": 0,
    "func-style": 0,
    "generator-star-spacing": [2, "before"],
    "guard-for-in": 1,
    "indent": [1, 2],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
    "keyword-spacing": [1, {"before": true, "after": true}],
    "max-depth": [2, 3],
    "max-nested-callbacks": [1, 5],
    "max-params": [2, 6],
    "max-statements": [1, 36],
    "new-cap": [1, {"newIsCap": true, "capIsNew": false}],
    "new-parens": 1,
    "newline-after-var": 0,
    "no-alert": 2,
    "no-array-constructor": 1,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-cond-assign": 2,
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-func-assign": 2,
    "no-implied-eval": 1,
    "no-inline-comments": 0,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 1,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [1, {"max": 2}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 1,
    "no-new-func": 2,
    "no-new-object": 1,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-octal-escape": 1,
    "no-octal": 1,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 1,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 1,
    "no-shadow": 2,
    "no-spaced-func": 1,
    "no-sparse-arrays": 2,
    "no-ternary": 0,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef-init": 2,
    "no-undef": 1,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "no-void": 2,
    "no-warning-comments": [0, {"terms": ["todo", "fixme"], "location": "start"}],
    "no-with": 2,
    "one-var": 0,
    "operator-assignment": 0,
    "padded-blocks": [1, "never"],
    "quote-props": [1, "consistent-as-needed"],
    "quotes": 0,
    "radix": 2,
    "semi-spacing": [1, {"before": false, "after": true}],
    "semi": [1, "always"],
    "sort-vars": 0,
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [0, {"anonymous": "always", "named": "never"}],
    "space-in-parens": [1, "never"],
    "space-infix-ops": 0,
    "space-unary-ops": [1, {"words": true, "nonwords": false}],
    "strict": 0,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "wrap-regex": 1
  }
}
